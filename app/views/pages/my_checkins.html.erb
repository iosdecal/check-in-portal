<%= form_tag checkins_path, method: "get" do %>
	<% if flash[:error] %>
    <div id="error_explanation">
      <h2>baddness: <%= flash[:error] %></h2>
    </div>
  <% end %>
  <%= label_tag("user_sid", "Enter your SID to see your checkins") %>
  <%= text_field_tag("user_sid") %>
  <%= submit_tag("My Checkins") %>
<% end %>
